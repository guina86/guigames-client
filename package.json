{
  "name": "gui-games",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "jest --passWithNoTests --no-cache",
    "test:watch": "npm t -- --watch",
    "test:staged": "npm t -- --findRelatedTests",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
    "generate": "plop --plopfile generators/plopfile.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint --fix",
      "npm run test:staged"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@styled-icons/boxicons-regular": "^10.34.0",
    "@styled-icons/boxicons-solid": "^10.34.0",
    "@styled-icons/fa-brands": "^10.34.0",
    "@styled-icons/material-outlined": "^10.34.0",
    "@styled-icons/remix-fill": "^10.18.0",
    "@types/lodash.xor": "^4.5.7",
    "graphql": "^15.5.1",
    "joi": "^17.6.0",
    "lodash.xor": "^4.5.0",
    "next": "12.2.0",
    "next-auth": "^3.27.0",
    "next-pwa": "^5.5.4",
    "nextjs-progressbar": "^0.0.14",
    "polished": "^4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-slick": "^0.29.0",
    "styled-components": "^5.3.5",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/node": "18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/react-slick": "^0.23.10",
    "@types/styled-components": "^5.1.25",
    "apollo": "^2.33.4",
    "eslint": "8.18.0",
    "eslint-config-next": "12.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-testing-library": "^5.5.1",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "jest-styled-components": "^7.0.8",
    "lint-staged": "^12.4.2",
    "plop": "^3.1.1",
    "prettier": "^2.7.1",
    "storybook-addon-next-router": "^4.0.0",
    "typescript": "4.7.4",
    "webpack": "^5.73.0"
  },
  "resolutions": {
    "graphql": "^15.5.1",
    "**/graphql": "^15.5.1"
  }
}
